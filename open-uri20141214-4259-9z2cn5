  <div class="node__content node--full--content node-article--full">
    <div itemprop="articleBody">
            <div class="b-node--full-body-content">
        <div id="block-maf-gigya-maf-gigya-sharebar" class="block block--maf-gigya block--maf-gigya-maf-gigya-sharebar even block-count-4">
        <div class="block__content">
    <div id="gigya-sharebar">
  <div class="counter"></div>
  <ul class="b-gigya-share--items-list">
    <li class="b-gigya-share--list--item">
